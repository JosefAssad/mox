# Connection parameters

# The host and port of the RabbitMQ service
amqp.incoming.interface = localhost:5672

# The queueName to read commands from
amqp.incoming.queueName = uploaded

# The credentials to use when authing to RabbitMQ
amqp.incoming.username = guest
amqp.incoming.password = guest


# The host and port of the RabbitMQ service
amqp.outgoing.interface = localhost:5672

# The queueName to send messages to
amqp.outgoing.queueName = converted

# The credentials to use when authing to RabbitMQ
amqp.outgoing.username = guest
amqp.outgoing.password = guest

